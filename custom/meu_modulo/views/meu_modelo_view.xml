<odoo>
    <record id="meu_modelo_view_form" model="ir.ui.view">
        <field name="name"> Meu modulo Form </field>
        <field name="model">meu.modelo</field>
        <field name="arch" type="xml">
            <form>

                <header>
                     <button string="Botão" type="object" name="imprimir" />
                 </header>

                <group>
                    <field name="name"/>
                    <field name="partner_ids" widget="many2many_tags"/>
                </group>

                <group>
                    <field name="date"/>
                </group>

            </form>
        </field>
    </record>

    <record id="meu_modelo_view_tree" model="ir.ui.view">
        <field name="name">Meu modulo Form</field>
        <field name="model">meu.modelo</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="date"/>
            </tree>
        </field>
    </record>

    <record id="meu_modelo_view_search" model="ir.ui.view">
        <field name="name">Meu modulo Search</field>
        <field name="model">meu.modelo</field>
        <field name="arch" type="xml">
            <search>
                <field name="name"/>
                <field name="partner_ids"/>
                <field name="date"/>
            </search>
        </field>
    </record>


    <act_window
        id="meu_modelo_action"
        name="Minhas ações"
        res_model="meu.modelo" />

    <menuitem
        id="meu_modelo_menu"
        name="Meu Menu"
        action="meu_modelo_action"
        parent=""
        sequence="5" />

    <record id="meu_modelo_view_form" model="ir.ui.view">
        <field name="name"> Meu modulo Form </field>
        <field name="model">meu.modelo_teste</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="title"/>
                    <field name="author" widget="many2many_tags"/>
                    <field name="editora" widget="many2many_tags"/>
                </group>
            </form>
        </field>
    </record>

    <act_window
            id="meu_modelo_teste_action"
            name="Meus Livros"
            res_model="meu.modelo_teste" />

    <menuitem id="meu_modelo_teste_menu" name="Livros" action="meu_modelo_teste_action" parent="" sequence="6"/>

</odoo>